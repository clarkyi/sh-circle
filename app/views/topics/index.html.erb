<p id="notice"><%= notice %></p>

<h1>Topics</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Company link</th>
      <th>Content</th>
      <th>String</th>
      <th>Title</th>
      <th>Tag</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @topics.each do |topic| %>
      <tr>
        <td><%= topic.user_id %></td>
        <td><%= topic.company_link %></td>
        <td><%= topic.content %></td>
        <td><%= topic.String %></td>
        <td><%= topic.title %></td>
        <td><%= topic.tag %></td>
        <td><%= topic.String %></td>
        <td><%= link_to 'Show', topic %></td>
        <td><%= link_to 'Edit', edit_topic_path(topic) %></td>
        <td><%= link_to 'Destroy', topic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Topic', new_topic_path %>
